{
  "name": "ui",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "require": "./dist/ui.umd.cjs",
      "import": "./dist/ui.js"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/ui.umd.cjs",
  "module": "./dist/ui.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build:lib": "vue-tsc --noEmit && vite build --mode lib",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "vue": "^3.2.37",
    "vue-router": "4"
  },
  "devDependencies": {
    "@testing-library/vue": "^6.6.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vueuse/core": "^9.3.1",
    "jsdom": "^20.0.0",
    "sass": "^1.55.0",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.8",
    "unplugin-vue-router": "^0.2.3",
    "vite": "^3.1.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vitest": "^0.23.4",
    "vue-tsc": "^0.40.4"
  }
